 <% include ../partials/header %>
 <table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Tittle</th>
            <th>Price</th>
        
            <th><a name="" id="" class="btn btn-success" href="add" role="button">Add</a></th>
        </tr>
    </thead>
    <tbody id="myTable">
     <% products.forEach(function(product){ %>
        <tr>
            <td scope="row"><%=product.purchases.name %></td>
            <td><%=product.purchase_items.quantity %></td>
            <td><%=product.products.tags %></td>
            
           
        </tr>
      <% }); %>
    </tbody>
</table>





