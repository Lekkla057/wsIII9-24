 <% include ../partials/header %>
 <form action="/products/update" method="post">
<div class="form-group">
  <label for="">ID</label>
  <input  type="number"
    class="form-control" name="id" id="" aria-describedby="helpId" placeholder=""value="<%= product.product_id %>" disabled>
    <input type="hidden" value="<%= product.product_id %>"id="custId" name="id" >
  
  </div>
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text"
      class="form-control" name="title" id="" aria-describedby="helpId" placeholder=""value="<%= product.title %>">
    
  </div>
  <div class="form-group">
    <label for="Price">Price</label>
    <input type="number"
      class="form-control" name="price" id="" aria-describedby="helpId" placeholder=""value="<%= product.price %>">
    
    
</div>
<div class="form-group">
  <label for="">Created At</label>
        <input type="text"
          class="form-control" value="<%= time %>" id="" aria-describedby="helpId" placeholder="" disabled>
          <input type="hidden" value="<%= time %>"id="custId" name="time" >
        
      </div>
      
 

<button type="submit" class="btn btn-success">Save</button>

</from>


