 <% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>E-mail</th>
            <th>Password</th>
            <th>Edit</th>
            <th><a name="" id="" class="btn btn-success" href="user_add" role="button">Add</a></th>
        </tr>
    </thead>
    <tbody>
     <% users.forEach(function(user){ %>
        <tr>
            <td scope="row"><%=user.id %></td>
            <td><%=user.email %></td>
            <td><%=user.password %></td>
            <td><a name="" id="" class="btn btn-warning" href="users/<%=user.id %>" role="button">Edit</a></td>
            <td><a name="" id="" class="btn btn-danger" href="user_delete/<%= user.id %>" onclick="return confirm('Are you sure you want to delete?')"role="button">Delete</a></td>
        </tr>
      <% }); %>
    </tbody>
</table>





  <% include ../partials/footer %>